import xml.etree.ElementTree as ET

tree = ET.parse("ALL DATA H1.XML")
root = tree.getroot()

for moment in root.iter("moment"):  #gets all the attribute within the tag, in this case the moment's attributes
    print(moment.attrib)
    locations = [moment.get("locations")]
    print(locations) #unorganized at the moment
    for char in locations:
        if char == ";":
            print(locations[0:locations.index(char)])


